function script_phstopcontrols() {
	obj_ph.invincible = true;
	obj_ph.image_speed = 0;
	obj_ph.image_index = 0;
	obj_ph.lastkeydown = "";
	obj_ph.leftkeydown = false;
	obj_ph.rightkeydown = false;
}