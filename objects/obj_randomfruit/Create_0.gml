/// @description pick a random fruit
randomize();

fruit = irandom_range(1,4);

if fruit = 1 {
	instance_change(obj_cherry, false)};
	
if fruit = 2 {
	instance_change(obj_grape, false)};
	
if fruit = 3 {
	instance_change(obj_orange, false)};
	
if fruit = 4 {
	instance_change(obj_melon, false)};